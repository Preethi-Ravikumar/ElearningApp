name: dockercompose17918427093593625862
services:
  db:
    container_name: elearning-db
    environment:
      MYSQL_ROOT_PASSWORD: mysql@123
    image: mysql:latest
    networks:
      elearningportal: null
    ports:
    - mode: ingress
      target: 3306
      published: "33060"
      protocol: tcp
    restart: always
  elearningportal:
    build:
      context: /Users/preethi/Projects/ElearningPortal
      dockerfile: ElearningPortal/Dockerfile
    container_name: elearning-portal
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_URLS: https://+:443;http://+:80
    image: elearningportal
    networks:
      elearningportal: null
    ports:
    - mode: ingress
      target: 80
      protocol: tcp
    - mode: ingress
      target: 443
      protocol: tcp
    - mode: ingress
      target: 8080
      published: "8005"
      protocol: tcp
    volumes:
    - type: bind
      source: /Users/preethi/.aspnet/https
      target: /root/.aspnet/https
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: /Users/preethi/.microsoft/usersecrets
      target: /root/.microsoft/usersecrets
      read_only: true
      bind:
        create_host_path: true
networks:
  elearningportal:
    name: dockercompose17918427093593625862_elearningportal